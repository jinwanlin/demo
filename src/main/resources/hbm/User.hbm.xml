<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE hibernate-mapping PUBLIC 	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"><hibernate-mapping>	<class name="com.demo.model.User" table="t_user" lazy="true">		<id name="id" type="java.lang.Long" >			<generator class="native" />		</id>		<property name="telephone" unique="true" not-null="true" />		<property name="name" unique="true" />								<property name="createTime" column="create_time" />		<property name="updateTime" column="update_time" />		 	</class></hibernate-mapping>